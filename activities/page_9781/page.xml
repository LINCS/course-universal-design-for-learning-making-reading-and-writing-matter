&lt;div&gt;
    &lt;p xml:lang="EN-US" lang="EN-US" paraid="0" paraeid="{92bd807c-e663-4153-b25e-a5b2c6ffe6ae}{126}" data-ccp-props="{&amp;quot;335551550&amp;quot;:1,&amp;quot;335551620&amp;quot;:1,&amp;quot;335559683&amp;quot;:0,&amp;quot;335559685&amp;quot;:0,&amp;quot;335559731&amp;quot;:0,&amp;quot;335559737&amp;quot;:0,&amp;quot;335562764&amp;quot;:2,&amp;quot;335562765&amp;quot;:1,&amp;quot;335562766&amp;quot;:4,&amp;quot;335562767&amp;quot;:0,&amp;quot;335562768&amp;quot;:4,&amp;quot;335562769&amp;quot;:6}"&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;&lt;/span&gt;&lt;/p&gt;
    &lt;div&gt;
        &lt;p xml:lang="EN-US" lang="EN-US" paraid="0" paraeid="{92bd807c-e663-4153-b25e-a5b2c6ffe6ae}{116}" data-ccp-props="{&amp;quot;335551550&amp;quot;:1,&amp;quot;335551620&amp;quot;:1,&amp;quot;335559683&amp;quot;:0,&amp;quot;335559685&amp;quot;:0,&amp;quot;335559731&amp;quot;:0,&amp;quot;335559737&amp;quot;:0,&amp;quot;335562764&amp;quot;:2,&amp;quot;335562765&amp;quot;:1,&amp;quot;335562766&amp;quot;:4,&amp;quot;335562767&amp;quot;:0,&amp;quot;335562768&amp;quot;:4,&amp;quot;335562769&amp;quot;:6}"&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;&lt;strong&gt;Video Title&lt;/strong&gt;:&amp;nbsp;&lt;/span&gt;
            &lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;Aydele Awobona and&amp;nbsp;&lt;/span&gt;&lt;span data-contrast="auto"&gt;&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;Connecting Education&amp;nbsp;with Family&lt;/span&gt;​&lt;/p&gt;
    &lt;/div&gt;
    &lt;div&gt;
        &lt;p xml:lang="EN-US" lang="EN-US" paraid="0" paraeid="{92bd807c-e663-4153-b25e-a5b2c6ffe6ae}{119}" data-ccp-props="{&amp;quot;335551550&amp;quot;:1,&amp;quot;335551620&amp;quot;:1,&amp;quot;335559683&amp;quot;:0,&amp;quot;335559685&amp;quot;:10,&amp;quot;335559731&amp;quot;:0,&amp;quot;335559737&amp;quot;:0,&amp;quot;335562764&amp;quot;:2,&amp;quot;335562765&amp;quot;:1,&amp;quot;335562766&amp;quot;:4,&amp;quot;335562767&amp;quot;:0,&amp;quot;335562768&amp;quot;:4,&amp;quot;335562769&amp;quot;:6}"&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;&lt;strong&gt;Duration:&lt;/strong&gt; 2&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;min 27sec&lt;/span&gt;​&lt;/p&gt;
    &lt;/div&gt;
    &lt;div&gt;
        &lt;p xml:lang="EN-US" lang="EN-US" paraid="0" paraeid="{92bd807c-e663-4153-b25e-a5b2c6ffe6ae}{122}" data-ccp-props="{&amp;quot;335551550&amp;quot;:1,&amp;quot;335551620&amp;quot;:1,&amp;quot;335559683&amp;quot;:0,&amp;quot;335559685&amp;quot;:10,&amp;quot;335559731&amp;quot;:0,&amp;quot;335559737&amp;quot;:0,&amp;quot;335562764&amp;quot;:2,&amp;quot;335562765&amp;quot;:1,&amp;quot;335562766&amp;quot;:4,&amp;quot;335562767&amp;quot;:0,&amp;quot;335562768&amp;quot;:4,&amp;quot;335562769&amp;quot;:6}"&gt;&lt;a href="https://lincs.ed.gov/sites/default/files/OCTAE%20Transcripts%20Module%204%20Literacy_0.docx" target="_blank"&gt;Downloadable Transcripts&lt;/a&gt;&lt;/p&gt;
    &lt;/div&gt;&lt;a href="https://www.youtube.com/watch?v=8HsO2jZFA0M" target="_blank"&gt;Aydele Awobona and Connecting Education with Family (video)&lt;/a&gt;&lt;br&gt;
    &lt;p&gt;&lt;/p&gt;
    &lt;p xml:lang="EN-US" lang="EN-US" paraid="0" paraeid="{92bd807c-e663-4153-b25e-a5b2c6ffe6ae}{126}" data-ccp-props="{&amp;quot;335551550&amp;quot;:1,&amp;quot;335551620&amp;quot;:1,&amp;quot;335559683&amp;quot;:0,&amp;quot;335559685&amp;quot;:0,&amp;quot;335559731&amp;quot;:0,&amp;quot;335559737&amp;quot;:0,&amp;quot;335562764&amp;quot;:2,&amp;quot;335562765&amp;quot;:1,&amp;quot;335562766&amp;quot;:4,&amp;quot;335562767&amp;quot;:0,&amp;quot;335562768&amp;quot;:4,&amp;quot;335562769&amp;quot;:6}"&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;Aydele&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;&amp;nbsp;&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;Awobona&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;&amp;nbsp;is an&amp;nbsp;Adult Learner at the&amp;nbsp;Chinese Mutual&amp;nbsp;&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;Aid&amp;nbsp;Association.&amp;nbsp;In this video&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;,&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;&amp;nbsp;&lt;/span&gt;
        &lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;Aydele&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;&amp;nbsp;shares her experience&amp;nbsp;&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;learning English in the&amp;nbsp;United States. Despite studying&amp;nbsp;&lt;/span&gt;
            &lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;English in Nigeria at&amp;nbsp;&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;the&amp;nbsp;&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;u&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;niversity&amp;nbsp;level&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;,&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;&amp;nbsp;&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;Aydele&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;&amp;nbsp;couldn't&amp;nbsp;read English. In this&amp;nbsp;&lt;/span&gt;
                &lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;video&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;,&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;&amp;nbsp;she&amp;nbsp;gives advice to those working with adults learning&amp;nbsp;&lt;/span&gt;
                    &lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;English. She&amp;nbsp;also shares&amp;nbsp;&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;how&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;&amp;nbsp;she is&amp;nbsp;motivated by the desire&amp;nbsp;&lt;/span&gt;
                        &lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;to model learning&amp;nbsp;for her children.&amp;nbsp;&lt;/span&gt;​&lt;/p&gt;
&lt;/div&gt;
&lt;div&gt;
